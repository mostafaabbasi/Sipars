        <script type="text/javascript" src="http://cdn.immex1.com/js/jspdf/plugins/jspdf.plugin.standard_fonts_metrics.js"></script>
        <script type="text/javascript" src="http://cdn.immex1.com/js/jspdf/plugins/jspdf.plugin.split_text_to_size.js"></script>
        <script type="text/javascript" src="http://cdn.immex1.com/js/jspdf/plugins/jspdf.plugin.from_html.js"></script>
        <script type="text/javascript" src="js/basic.js"></script>*@



    <script src="@Url.Content("~/assets/Assets/js/slimscroll/jquery.slimscroll.min.js")"></script>

    <!--Beyond Scripts-->
    <script src="@Url.Content("~/assets/Assets/js/skins.min.js")"></script>
    <script src="@Url.Content("~/assets/Assets/js/beyond.min.js")"></script>
    <script src="@Url.Content("~/assets/Assets/js/toastr/toastr.js")"></script>
    <script src="@Url.Content("~/assets/Assets/js/Customjs.js")"></script>
    <script src="@Url.Content("~/assets/Assets/js/select2/select2.min.js")"></script>

    <script type="text/javascript"> var containerCalender = "";</script>
    <script src="@Url.Content("~/assets/Assets/js/MdBootstrapPersianDateTimePicker/jquery.Bootstrap-PersianDateTimePicker.js")"></script>
    <script src="@Url.Content("~/assets/Assets/js/MdBootstrapPersianDateTimePicker/jalaali.js")"></script>
    <script src="@Url.Content("~/assets/Assets/js/fuelux/treeview/tree-custom.min.js")"></script>
    @*<script src="@Url.Content("~/assets/Assets/js/fuelux/treeview/treeview-init.js")"></script>*@
    <script src="@Url.Content("~/assets/Assets/js/datetime/bootstrap-timepicker.min.js")"></script>
    @*<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-sanitize.js"></script>*@
    <script src="@Url.Content("~/assets/Assets/js/angular/angular-sanitize/angular-sanitize.js")"></script>

    @*<script src="@Url.Content("~/assets/js/jspdf.debug.js")"></script>*@

