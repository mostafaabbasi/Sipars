    <link href="@Url.Content("~/assets/Assets/css/font-awesome.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/assets/Assets/css/weather-icons.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/assets/Assets/css/bootstrap-timepicker.min.css")" rel="stylesheet" />
