    <script src="@Url.Content("~/assets/Assets/js/jquery.dataTables.min.js")"></script>
    <script src="@Url.Content("~/assets/Assets/js/datatable/angular-datatables.min.js")"></script>
    <script src="@Url.Content("~/assets/Assets/js/datatable/dataTables.bootstrap.min.js")"></script>





    @*<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.1.135/jspdf.min.js"></script>*@
    @*<script type="text/javascript" src="http://cdn.uriit.ru/jsPDF/libs/adler32cs.js/adler32cs.js"></script>*@
