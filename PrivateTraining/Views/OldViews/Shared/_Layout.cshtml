    <link href="@Url.Content("~/assets/Assets/css/Datatable/dataTables.foundation.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/assets/Assets/css/Datatable/dataTables.jqueryui.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/assets/Assets/js/jquery.dataTables.min.js")" rel="stylesheet" />
    <!-- datatble Class -->
